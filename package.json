{
  "name": "react-future-stack",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "history": "^4.6.2",
    "polished": "^1.1.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "remove": "^0.1.5",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.5",
    "styled-components": "^2.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "add": "^2.0.6",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "browser-sync": "^2.18.12",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.28.4",
    "ejs": "^2.5.6",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.2.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.48.0",
    "front-matter": "^2.1.2",
    "highlight.js": "^9.10.0",
    "immutable": "^3.8.1",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "markdown-it": "^8.3.1",
    "mocha": "^3.2.0",
    "path-to-regexp": "^1.7.0",
    "pixrem": "^3.0.2",
    "pleeease-filters": "^4.0.0",
    "prettier-eslint": "^6.3.0",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "3.0.0-beta.7",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "s3": "^4.4.0",
    "style-loader": "^0.18.2",
    "stylelint": "^7.11.1",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": "last 2 versions"
          },
          "loose": true,
          "modules": false
        }
      ],
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-runtime",
      [
        "lodash",
        {
          "id": [
            "lodash",
            "semantic-ui-react"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single"
    }
  },
  "scripts": {
    "eslint": "eslint components src test tools postcss.config.js webpack.config.js",
    "stylelint": "stylelint \"components/**/*.css\" \"src/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "build": "node tools/build.js",
    "build:debug": "node tools/build.js --debug",
    "publish": "node tools/publish.js",
    "publish:debug": "node tools/publish.js --debug",
    "start": "node tools/run.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
